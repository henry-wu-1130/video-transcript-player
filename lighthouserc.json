{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:4173"
      ],
      "numberOfRuns": 1
    },
    "assert": {
      "preset": "lighthouse:recommended"
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./lhci-report",
      "reportFilenamePattern": "report.html"
    }
  }
}